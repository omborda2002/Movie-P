<script>
  let count = 0;
  function btnClick() {
    if (count== 0) {
      count++;
      document.querySelector('.thenn').style.display = "block";
    }else{
      count = 0;
      document.querySelector('.thenn').style.display = "none";
    }
  }

  function openNav() {
    document.getElementById("mySidenav").style.width = "250px";
    document.querySelector(".container-sm").style.marginLeft = "250px";
    document.body.style.backgroundColor = "rgba(0,0,0,0.8)";
  }

  function closeNav() {
    document.getElementById("mySidenav").style.width = "0";
    document.querySelector(".container-sm").style.marginLeft = "0";
    document.body.style.backgroundColor = "white";
  }
</script>


